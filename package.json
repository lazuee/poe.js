{
	"description": "A package for interacting with AI models from poe.com",
	"name": "@lazuee/poe.js",
	"version": "0.1.8",
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"graphql",
		"LICENSE.md",
		"README.md"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"scripts": {
		"build": "unbuild"
	},
	"unbuild": {
		"declaration": true,
		"rollup": {
			"emitCJS": true
		},
		"entries": [
			"src/index"
		]
	},
	"homepage": "https://github.com/lazuee/poe.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lazuee/poe.js.git"
	},
	"bugs": {
		"url": "https://github.com/lazuee/poe.js/issues"
	},
	"keywords": [
		"poe",
		"ai",
		"chatgpt"
	],
	"contributors": [
		"lazuee <lazuee.dev@gmail.com>"
	],
	"publishConfig": {
		"access": "public",
		"tag": "latest"
	},
	"devDependencies": {
		"typescript": "5.1.6",
		"unbuild": "1.2.1"
	},
	"dependencies": {
		"@types/md5": "2.3.2",
		"@types/node": "20.4.1",
		"@types/ws": "8.5.5",
		"axios": "1.4.0",
		"dotenv": "16.3.1",
		"md5": "2.3.0",
		"p-queue-compat": "1.0.223",
		"random-useragent": "0.5.0",
		"ws": "8.13.0"
	},
	"packageManager": "pnpm@8.3.1"
}
